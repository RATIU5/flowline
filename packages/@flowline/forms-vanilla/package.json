{
  "name": "@flowline/forms-vanilla",
  "version": "0.0.1",
  "description": "Vanilla TypeScript/JavaScript adapter for @flowline/forms",
  "keywords": [
    "effect",
    "forms",
    "typescript",
    "validation",
    "reactive"
  ],
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "clean": "rm -rf dist",
    "dev": "bun run build:watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run clean && bun run build"
  },
  "type": "module",
  "main": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mod.js",
      "types": "./dist/mod.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "peerDependencies": {
    "effect": "^3.17.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@effect/platform": "^0.90.0",
    "@flowline/forms": "workspace:*"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RATIU5/flowline.git",
    "directory": "packages/@flowline/forms-vanilla"
  },
  "bugs": {
    "url": "https://github.com/RATIU5/flowline/issues"
  },
  "homepage": "https://github.com/RATIU5/flowline#readme",
  "license": "MIT"
}
