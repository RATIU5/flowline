{
  "name": "@flowline/forms-vanilla",
  "version": "0.0.1",
  "description": "Vanilla TypeScript/JavaScript adapter for @flowline/forms",
  "keywords": [
    "effect",
    "forms",
    "typescript",
    "validation",
    "reactive"
  ],
  "scripts": {
    "build": "buildkit build src/**/*.ts",
    "dev": "buildkit dev src/**/*.ts",
    "typecheck": "tsgo --noEmit"
  },
  "type": "module",
  "main": "./src/mod.ts",
  "types": "./src/mod.ts",
  "exports": {
    ".": {
      "import": "./src/mod.ts",
      "types": "./src/mod.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "peerDependencies": {
    "effect": "^3.17.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@effect/platform": "^0.90.0",
    "@flowline/forms": "workspace:*"
  },
  "devDependencies": {
    "@flowline/buildkit": "workspace:*",
    "bun-types": "latest",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RATIU5/flowline.git",
    "directory": "packages/@flowline/forms-vanilla"
  },
  "bugs": {
    "url": "https://github.com/RATIU5/flowline/issues"
  },
  "homepage": "https://github.com/RATIU5/flowline#readme",
  "license": "MIT"
}
